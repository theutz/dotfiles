#compdef edit
# @generated by usage-cli from /Users/michael/.config/mise/tasks/edit.kdl
local curcontext="$curcontext"

_edit() {
  typeset -A opt_args
  local curcontext="$curcontext" cache_policy

  if ! type -p usage &> /dev/null; then
      echo >&2
      echo "Error: usage CLI not found. This is required for completions to work in edit." >&2
      echo "See https://usage.jdx.dev for more information." >&2
      return 1
  fi

  local spec_file="${TMPDIR:-/tmp}/usage__usage_spec_edit.spec"
  cat > "$spec_file" <<'__USAGE_EOF__'
name edit
bin edit
arg <program>
complete program run="mise run edit list"
cmd completions {
    arg <shell> {
        choices zsh
    }
}
__USAGE_EOF__
  _arguments "*: :(($(command usage complete-word --shell zsh -f "$spec_file" -- "${words[@]}" )))"
  return 0
}

if [ "$funcstack[1]" = "_edit" ]; then
    _edit "$@"
else
    compdef _edit edit
fi

