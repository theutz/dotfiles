:: default : echo 'default mode'; \
  borders active_color=0xfff6c177 \
    inactive_color=0x88191724 \
    width=8.0
:: win @ : echo 'win mode'; \
  borders active_color=0xfff6c177 \
    inactive_color=0xffb4637a \
    width=12.0

default < alt - m ; win
default < cmd + ctrl + alt - r : yabai --restart-service

win < 1 : yabai -m space --focus 1
win < 2 : yabai -m space --focus 2
win < 3 : yabai -m space --focus 3
win < 4 : yabai -m space --focus 4
win < 5 : yabai -m space --focus 5
win < 6 : yabai -m space --focus 6
win < 7 : yabai -m space --focus 7
win < 8 : yabai -m space --focus 8
win < 9 : yabai -m space --focus prev || yabai -m space --focus first
win < 0 : yabai -m space --focus next || yabai -m space --focus last
win < b : yabai -m window --focus prev
win < e : yabai -m space --equalize
win < escape ; default
win < f : yabai -m window --focus next
win < h : yabai -m window --focus west || yabai -m display --focus west
win < j : yabai -m window --focus south
win < k : yabai -m window --focus north
win < l : yabai -m window --focus east || yabai -m display --focus east
win < n : yabai -m window --focus stack.next
win < o : yabai -m window --ratio abs:0.7
win < p : yabai -m window --focus stack.prev
win < r : yabai -m space --rotate 270
win < return : yabai -m window --toggle zoom-fullscreen
win < s : typeset -A layouts; \
  layouts=([bsp]=stack [float]=stack [stack]=bsp); \
  layout="$(yabai -m query --spaces --space | jq -r .type)"; \
  yabai -m space --layout $layouts[$layout]
win < tab : yabai -m display --focus next || yabai -m display --focus first
win < x : yabai -m space --mirror x-axis
win < y : yabai -m space --mirror y-axis

win < alt - h : for size in left:-50:0 right:-50:0; do yabai -m window --resize $size; done
win < alt - j : for size in bottom:0:50 top:0:50; do yabai -m window --resize $size; done
win < alt - k : for size in bottom:0:-50 top:0:-50; do yabai -m window --resize $size; done
win < alt - l : for size in left:50:0 right:50:0; do yabai -m window --resize $size; done
win < alt - 9 : yabai -m space --destroy last
win < alt - 0 : yabai -m space --create

win < ctrl - h : yabai -m window --warp west
win < ctrl - j : yabai -m window --warp south
win < ctrl - k : yabai -m window --warp north
win < ctrl - l : yabai -m window --warp east
win < ctrl - n : yabai -m window --stack next
win < ctrl - p : yabai -m window --stack prev

win < shift - 1 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m window --space 1; \
  yabai -m window --focus $id
win < shift - 2 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m window --space 2; \
  yabai -m window --focus $id
win < shift - 3 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m window --space 3; \
  yabai -m window --focus $id
win < shift - 4 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m window --space 4; \
  yabai -m window --focus $id
win < shift - 5 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m window --space 5; \
  yabai -m window --focus $id
win < shift - 6 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m window --space 6; \
  yabai -m window --focus $id
win < shift - 7 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m window --space 7; \
  yabai -m window --focus $id
win < shift - 8 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m window --space 8; \
  yabai -m window --focus $id
win < shift - 9 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m space --move prev; \
  yabai -m window --focus $id
win < shift - 0 : id="$(yabai -m query --windows --window | jq -r .id)"; \
  yabai -m space --move next; \
  yabai -m window --focus $id
win < shift - b : yabai -m window --swap prev
win < shift - f : yabai -m window --swap next
win < shift - h : yabai -m window --swap west
win < shift - j : yabai -m window --swap south
win < shift - k : yabai -m window --swap north
win < shift - l : yabai -m window --swap east
win < shift - n : yabai -m window --swap stack.next
win < shift - p : yabai -m window --swap stack.prev
win < shift - r : yabai -m space --rotate 90
win < shift - return : yabai -m window --toggle zoom-parent

